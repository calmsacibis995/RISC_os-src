#include "sys/sbd.h"

/*
 * Sable console information.  Indexed on machine_type.
 */

#define SCONS0_BASE_R2300	0x1f000000
#define SCONS0_BASE_R2400	0x18090000
#define SCONS0_BASE_R3200	0x18090000
#define SCONS0_BASE_R6300	0x1f000000

#define SCONS1_BASE_R2300	0x1f000000
#define SCONS1_BASE_R2400	0x180a0000
#define SCONS1_BASE_R3200	0x180a0000
#define SCONS1_BASE_R6300	0x1f000000

int SCONS0_BASE[] = {
	PHYS_TO_K1(SCONS0_BASE_R2300),		/* M500 */
	PHYS_TO_K1(SCONS0_BASE_R2300),		/* M800 */
	PHYS_TO_K1(SCONS0_BASE_R2300),		/* M1000 */
	PHYS_TO_K1(SCONS0_BASE_R2400),		/* Intrepid */
	PHYS_TO_K1(SCONS0_BASE_R3200),		/* M2000 */
	PHYS_TO_K1(SCONS0_BASE_R6300)
	};

int SCONS1_BASE[] = {
	PHYS_TO_K1(SCONS1_BASE_R2300),		/* M500 */
	PHYS_TO_K1(SCONS1_BASE_R2300),		/* M800 */
	PHYS_TO_K1(SCONS1_BASE_R2300),		/* M1000 */
	PHYS_TO_K1(SCONS1_BASE_R2400),		/* Intrepid */
	PHYS_TO_K1(SCONS1_BASE_R3200),		/* M2000 */
	PHYS_TO_K1(SCONS1_BASE_R6300)
	};


