	id="RISC/os System V Commands and Libraries Source"
	optional
	splitboms="src_dirs src_files"
	bom=usr-src

subpackage=bsd43-src
	id="RISC/os 4.3 BSD Commands and Libraries Source"
	optional
	splitboms="bsd43_src_dirs bsd43_src_files"
	bom=bsd43-src

subpackage=posix-src
	id="RISC/os POSIX 1003.1 Libraries and Include File Source"
	optional
	splitboms="posix_src_dirs posix_src_files"
	bom=posix-src

