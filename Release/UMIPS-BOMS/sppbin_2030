stand drwxr-xr-x bin bin 2
etc drwxr-xr-x bin bin 2
stand/format.2030 -r-xr-xr-x bin bin 2
stand/sash.2030 -r-xr-xr-x bin bin 2
etc/bfsd l--------- bin bin 1 ../usr/etc/bfsd
usr/etc/bfsd -r-xr-xr-x bin bin 1
stand/format -r-xr-xr-x bin bin 2 stand/format.2030
stand/sash -r-xr-xr-x bin bin 2 stand/sash.2030
